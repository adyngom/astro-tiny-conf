<script type="text/javascript">
  (function (win, doc) {
    win.addEventListener("load", function () {
      const bodyJS = doc.querySelector(".js > body");
      if (bodyJS) {
        bodyJS.classList.add("render");
      }
      const innerContent = doc.querySelector(".content__inner");
      if (innerContent) {
        innerContent.style.opacity = 1;
      }
    });
  })(window, document);
</script>
