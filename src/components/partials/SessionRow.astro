---
import type { Session, Speaker } from "../../interfaces";

const { title, speakers, description, date, time } = Astro.props;
---

<div class="col-md-3">
  <div class="schedule-time mb-4">
    <p>tbd - tbd</p>
  </div>
</div>
<div class="col-md-9 mt-4 mt-md-0">
  <h5>{title}</h5>
  <p><em>{speakers.map((speaker: Speaker) => speaker.name).join(", ")}</em></p>
  <div class="section mt-2">
    <p>
      {description}
    </p>
  </div>
</div>
<div class="col-md-12">
  <div class="title-page-line mt-4 mb-5"></div>
</div>
